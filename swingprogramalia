    private void btnCalcularActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_btnCalcularActionPerformed
        String nombre = txtNombre.getText();
        double sueldo = Double.parseDouble(txtSueldo.getText());
        double impuesto;
        double afp = 0.11;
        double salud = 0.07;

        if (sueldo >= 10000 && sueldo <= 30000) {
            impuesto = 0.10;
        } else if (sueldo > 30000 && sueldo <= 50000) {
            impuesto = 0.2;
        } else if (sueldo > 50000) {
            impuesto = 0.35;
        } else {
            impuesto = 0;
        }


        double resultadoImpuesto = sueldo * impuesto;
        double resultadoAFP = sueldo * afp;
        double resultadoSalud = sueldo * salud;
        double resultadoSueldoLiquido = sueldo - resultadoImpuesto - resultadoAFP - resultadoSalud;

        txtImpuesto.setText(String.format("%.2f", resultadoImpuesto));
        txtAFP.setText(String.format("%.2f", resultadoAFP));
        txtSalud.setText(String.format("%.2f", resultadoSalud));
        txtSueldoLiquido.setText(String.format("%.2f", resultadoSueldoLiquido));
        txtResumen.setText("");

        String puestoTrabajo = cboxEmpleado.getSelectedItem().toString();
        String resumen = String.format("Buen día, %s. Su puesto de trabajo es %s.\n", nombre, puestoTrabajo);
        resumen += String.format("Debe pagar $%.0f de Impuestos.\n", resultadoImpuesto);
        resumen += String.format("Debe pagar $%.0f a su AFP.\n", resultadoAFP);
        resumen += String.format("Debe pagar $%.0f de Salud.\n", resultadoSalud);
        resumen += String.format("Por lo tanto, su sueldo líquido es $%.0f.", resultadoSueldoLiquido);

        txtResumen.setText(resumen);
    }//GEN-LAST:event_btnCalcularActionPerformed

